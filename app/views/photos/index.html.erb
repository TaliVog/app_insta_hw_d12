<h1>Showing photos...</h1>
<%= link_to "New photo", new_user_photo_path(current_user) %><br><br>

<% @photos.each do |photo| %>

	<%= photo.caption %><br>
	<%= link_to 'Show', user_photo_path(current_user, photo)  %>
	<%= link_to 'Edit', edit_user_photo_path(current_user, photo) %>
	<%= link_to 'Destroy', user_photo_path(current_user, photo),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
<% end %>